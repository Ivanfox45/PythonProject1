   /*ВЕРСИИ И РЕВИЗИИ*/
   
   jQuery('.rev_hid a').click(function(){
       if (jQuery(this).hasClass('active')) {
           jQuery(this).parents('.rev_hid').prev('.sas_slide').find('.file_list.revis').addClass('hiddme');
           jQuery(this).text('Показать ревизии файлов');
           jQuery(this).removeClass('active');
       } else {
           jQuery(this).parents('.rev_hid').prev('.sas_slide').find('.file_list').removeClass('hiddme');
           jQuery(this).text('Скрыть ревизии файлов');
           jQuery(this).addClass('active');
       }
       return false;
   })
   
