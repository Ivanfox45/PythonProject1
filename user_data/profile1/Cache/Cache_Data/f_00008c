    var rt = jQuery(this).attr('data-p');
    jQuery('.comviews').removeClass('active');
    jQuery(this).addClass('active');
    jQuery('.companeall_one').removeClass('active');
    jQuery('.companeall_one[data-p="'+rt+'"]').addClass('active');
    
    return false;
})


