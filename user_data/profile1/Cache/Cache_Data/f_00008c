jQuery('.reti a').click(function(){
    jQuery('#viewsa').submit();
    return false;
})



jQuery('.filone.hover .fills').click(function(){
     jQuery(this).next('.ctnfillnew').toggleClass('active');   
     return false;
})
  jQuery(document).mouseup(function (e){
         
     if (!jQuery(".ctnfillnew").is(e.target) &&
      jQuery(".ctnfillnew").has(e.target).length === 0) {
      jQuery(".ctnfillnew").removeClass('active'); // скрываем блок
     }
  });  


 
jQuery('.visall').click(function(){
    
    jQuery(this).parents('.sas_slide').find('.file_list').removeClass('hidd');
    jQuery(this).remove();
    return false;
})


jQuery('.morefile').click(function(){
    jQuery('.file_one').removeClass('hiddens');   
      
    
    jQuery(this).remove();
    return false;
})
 
jQuery('#ddf, #dmf, #dyf,#ddf11, #dmf11, #dyf11,#ddf12, #dmf12, #dyf12').change(function(){
    validates();
})
