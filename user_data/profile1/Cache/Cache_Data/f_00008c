    jQuery('.page_hidd a').click(function(){
    if (jQuery(this).hasClass('active')) {
        jQuery(this).removeClass('active');
        jQuery(this).text('Скрыть карту');
        jQuery('.page_map').removeClass('hidden');
    } else {
        jQuery(this).addClass('active');
        jQuery(this).text('Показать карту');
        jQuery('.page_map').addClass('hidden');
    }
    return false;
})

jQuery('#select_inner').chosen();
 
jQuery('.navnewitem a').click(function(){
    at = jQuery(this).attr('href');
    jQuery('.navnewitem a').removeClass('active');
    jQuery('.tab-pane1').removeClass('showsactive');
    jQuery('.tab-pane1').addClass('nones');
    jQuery('.npp'+at).removeClass('nones');
    jQuery('.npp'+at).addClass('showsactive');
    
    jQuery(this).addClass('active');
     return false;
})


    
    
    
    
    
    
    
    
    
    
    jQuery('#selectone').change(function(){
    alp = jQuery(this).val();
    
    jQuery.post('/wp-admin/admin-ajax.php', {'action':'selectupr', 'alp':alp }, function(response){
          jQuery('#select_inner').html(response);
          jQuery('#select_inner').val('').trigger('chosen:updated');
           
     }); 
    
})

jQuery('#selectman').change(function(){
    alp = jQuery(this).val();
        jQuery.post('/wp-admin/admin-ajax.php', {'action':'selectman', 'alp':alp }, function(response){
          console.log(response);
          jQuery('#select_inner').html(response);
          jQuery('#select_inner').val('').trigger('chosen:updated');
           
     }); 
    
})




jQuery('#selectoneone').change(function(){
    
    alp = jQuery(this).val();
    if (alp == '') {
        alp = jQuery('#selectoneone').attr('data-z');
    }
    jQuery.post('/wp-admin/admin-ajax.php', {'action':'selectinner', 'alp':alp }, function(response){
          jQuery('#select_inner').html(response);
           jQuery('#select_inner').val('').trigger('chosen:updated');
     }); 
    
})
    
   
   
jQuery('.step_search_select_one').click(function(){
   var art = jQuery(this).attr('data-p');
   jQuery('.step_search_select_one, .step_search_pane_one.start').removeClass('active');
   //jQuery('#edit-field-npa-type option[value=0], #edit-field-npa-accepted-by option[value=0],  #edit-field-npa-class-parents-all-ul option[value=0], #edit-field-npa-class-parents-all option[value=0]').prop('selected', true);
   jQuery(this).addClass('active');
   if (art == 'npa-te') {
       jQuery('.step_search_pane_one.start[data-p="npa-te"]').addClass('active');
       jQuery('#step').val(1);
   }
   if (art == 'npa-type') {
       jQuery('.step_search_pane_one.start[data-p="npa-type"]').addClass('active');
       jQuery('#step').val(2);
   }
   if (art == 'npa-organ') {
       jQuery('.step_search_pane_one.start[data-p="npa-organ"]').addClass('active');
       jQuery('#step').val(3);
   }
   jQuery('.visibh').addClass('hidd');
   jQuery('.hiom').addClass('active');
   jQuery('.rv_form').removeClass('active');
});
jQuery('.list_one').click(function(){
   var art = jQuery(this).attr('data-list');
   var datap = jQuery(this).attr('data-p');
 
   jQuery('.list_one').removeClass('active');
   jQuery(this).addClass('active');
   
   
   if (datap == 'npa-te') {
       jQuery('#edit-field-npa-class-parents-all-ul option[value='+art+']').prop('selected', true);
       jQuery('#views-exposed-form-npa-search-page').submit();
       jQuery('#edit-field-npa-type option[value=0], #edit-field-npa-accepted-by option[value=0], #edit-field-npa-class-parents-all option[value=0],  #edit-field-npa-accepted-by option[value=0]').prop('selected', true);
   }
   if (datap == 'npa-te-pod') {
       jQuery('#edit-field-npa-class-parents-all option[value='+art+']').prop('selected', true);
       jQuery('#step').val(2);
       jQuery('#views-exposed-form-npa-search-page').submit();
   }
   if (datap == 'npa-type') {
        var art = jQuery(this).attr('data-z');
        jQuery('#edit-field-npa-type option[data-npa='+art+']').prop('selected', true);
      //  jQuery('#edit-field-npa-accepted-by option[value=0]').prop('selected', true);
    //    jQuery('#edit-field-npa-accepted-by option[value=0], #edit-field-npa-class-parents-all option[value=0], #edit-field-npa-class-parents-all-ul option[value=0]').prop('selected', true);
         
          jQuery('#step').val(3);
   jQuery('#views-exposed-form-npa-search-page').submit();
    }
   if (datap == 'npa-organ') {
    var atz = jQuery(this).attr('data-z');
    jQuery('#edit-field-npa-accepted-by option[data-org='+atz+']').prop('selected', true);
    jQuery('#step').val(0);
    jQuery('#views-exposed-form-npa-search-page').submit();
    }
    
    if (datap == 'npa-org-sel') {
       var atz = jQuery(this).attr('data-z');
       jQuery('#edit-field-npa-accepted-by option[value="'+atz+'"]').prop('selected', true);
       jQuery('#step').val(0);
       jQuery('#views-exposed-form-npa-search-page').submit();
    }
    if (datap == 'npa-ty-sel') {
       var atz = jQuery(this).attr('data-z');
       jQuery('#edit-field-npa-type option[value="'+atz+'"]').prop('selected', true);
       jQuery('#step').val(0);
       jQuery('#views-exposed-form-npa-search-page').submit();
    }
    
    
   
});
   
        

jQuery('.rv_form').click(function(){
    if (jQuery(this).hasClass('active')) {
    jQuery('.visibh').addClass('hidd');
    jQuery(this).removeClass('active');    
    } else {
        jQuery('.visibh').removeClass('hidd');
    jQuery(this).addClass('active');
    }
    
});






