 jQuery('.expandable-title').click(function(){
        jQuery('.expandable-section').removeClass('open');
        if (jQuery(this).hasClass('open')) {
        jQuery(this).parent().removeClass('open');
        jQuery(this).removeClass('open');    
        } else {
        jQuery(this).parent().addClass('open');
        jQuery(this).addClass('open');
        
        jQuery('.expandable-title-new').removeClass('open');
        }
    })
    
 jQuery('.expandable-title-new').click(function(){
       
             //jQuery('.expandable-ser').removeClass('open');
       
       if (jQuery(this).hasClass('open')) {
           
        jQuery(this).parent().removeClass('open');
        jQuery(this).removeClass('open');    
        
        } else {
        
         jQuery(this).parent().addClass('open');
         jQuery(this).addClass('open');
        }
    })    
var fr = jQuery(window).width();
        if (fr > 1000) {
        jQuery(document).mouseup( function(e){ // событие клика по веб-документу
		var div = jQuery( ".sub-menu" ); // тут указываем ID элемента
		if ( !div.is(e.target) // если клик был не по нашему блоку
		    && div.has(e.target).length === 0 ) { // и не по его дочерним элементам
			jQuery( ".sub-menu" ).removeClass('active');
		}
	});
    }
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
         jQuery('#edit-field-npa-class-parents-all-ul').change(function(){
             npa = jQuery('#edit-field-npa-class-parents-all-ul').val();
              jQuery.post('/wp-admin/admin-ajax.php', {'action':'npa_load', 'npa':npa}, function(response){
                 jQuery('#select_np').html(response);
                 }); 
            
            
        });  
    
  
    
    
    
    
