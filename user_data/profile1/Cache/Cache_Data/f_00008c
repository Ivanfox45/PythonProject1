  jQuery('.ddls').click(function(){
     var fls = jQuery(this).attr('data-f');
     var atr = jQuery(this).attr('href');
     jQuery.post('/wp-admin/admin-ajax.php', {'action':'download', 'fls':fls  }, function(response){
         document.location.href= atr;
    });
    return false;
})
    
 
    jQuery('.clickbydown a').click(function(){
     var fls = jQuery('.pagepid').val();
     
     var href = this.href;
     
     if(/\.(pdf|xlsx|csv|docx|zip)$/i.test(href)) {
        jQuery.post('/wp-admin/admin-ajax.php', {'action':'downloadpage', 'fls':fls  }, function(response){
         document.location.href= href;
     });
     } else {
         if (!href.indexOf("#") != -1) {
           
          } else {
               document.location.href= href;
          }
     }
     
     
 })
 
 
