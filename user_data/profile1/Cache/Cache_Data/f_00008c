jQuery('.sendopr').click(function(){
     var allvals = jQuery('.allval').val();   
     var emails = jQuery('.ems').val();   
     var opr = jQuery(this).attr('data-opr');
     var idk = jQuery(this).attr('data-idk');
     jQuery.post('/wp-admin/admin-ajax.php', {'action':'saveuseropros', 'allvals':allvals, 'emails':emails, 'opr':opr, 'idk':idk}, function(response){
       jQuery('.resultsop').html('<h3>Спасибо что приняли участие в нашем опросе.</h3>');
     });      
     return false;
})
jQuery('.sft').click(function(){
 
     var quiz = jQuery(this).attr('data-o');
     jQuery.post('/wp-admin/admin-ajax.php', {'action':'genquiz', 'quiz':quiz }, function(response){
      jQuery('.loads').removeClass('active');
       jQuery('[data-o="'+quiz+'"]').prev('.loads').addClass('active');
     });      
     return false;
})









 
function allval() {
    var all = '';
    jQuery('.stepval').each(function(){
        if (jQuery(this).val() != '') {
        all += jQuery(this).val()+'@@';
    } else {
        all += 'Не указан ответ@@';
    }
    })
    jQuery('.allval').val(all);
} 
 
 
jQuery('.multi_choose').click(function(){
     jQuery(this).toggleClass('active');
     var op = '';
     jQuery('.multi_choose.active').each(function(){
         op += jQuery(this).text()+'##';
     })
     jQuery(this).parents('.oprus_one').find('.stepval').val(op); 
     return false;
})

jQuery('.onlytext').on('keyup', function(){
     var nval = jQuery(this).val();
      jQuery(this).parents('.oprus_one').find('.stepval').val(nval); 
})

jQuery('.next_op').click(function(){
     var vals = jQuery(this).parents('.oprus_one').find('.stepval').val(); 
     if (jQuery(this).parents('.oprus_one').find('.stepval').hasClass('requi')) {
      if (vals != '') {
        var n = jQuery(this).attr('data-n');
        jQuery('.oprus_one').removeClass('active');   
        jQuery('.oprus_one[data-o="'+n+'"]').addClass('active');   
        allval();
        } else {
        jQuery(this).parents('.oprus_one').find('.err').addClass('active');
        }   
     } else {
        var n = jQuery(this).attr('data-n');
        jQuery('.oprus_one').removeClass('active');   
        jQuery('.oprus_one[data-o="'+n+'"]').addClass('active');   
        allval();  
     }
     
     return false;
})

jQuery('.opsels').on('change', function(){
     var nval = jQuery(this).val();
     if ( nval == 0) {
         jQuery(this).parents('.oprus_one').find('.stepval').val(''); 
     } else {
         jQuery(this).parents('.oprus_one').find('.stepval').val(nval); 
     }
     
})


jQuery('.prev_op').click(function(){
     var n = jQuery(this).attr('data-p');
     jQuery('.oprus_one').removeClass('active');   
     jQuery('.oprus_one[data-o="'+n+'"]').addClass('active');   
     allval();
      jQuery('.err').removeClass('active');
     return false;
})



        
