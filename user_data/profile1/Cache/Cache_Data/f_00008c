})

   jQuery('.anonsblotexthiddmore span').click(function(){
       
       if (jQuery(this).hasClass('active')) {
           jQuery(this).removeClass('active');
           jQuery('.anonsblotexthidd').removeClass('active');
           jQuery(this).text('Посмотреть');
       } else {
           
           jQuery(this).addClass('active');
           jQuery('.anonsblotexthidd').addClass('active');
           jQuery(this).text('Скрыть');
       }
       return false;
   })
   

jQuery('.sid_one.opens').click(function(){
    
    atr = jQuery(this).attr('data-v');
    poll = jQuery(this).attr('data-pol');
    uid = jQuery('.orp').val();
    if (atr == 'bad') { te = 'Извините! Мы постараемся исправиться('; cla = 'bad'; }
    if (atr == 'mid') { te = 'Понимаем что нам надо еще поработать дополнительно!'; cla = 'mid'; }
    if (atr == 'good') {te = 'Отлично! Мы рады что Вам все понравилось!'; cla = 'good';}
    
    
             jQuery.post('/wp-admin/admin-ajax.php', {'action':'pollsn', 'poll':poll, 'atr':atr, 'uid':uid }, function(response){
          
           
     });  
    
    
    jQuery(this).parents('.oprone_select_test_values').next('.oprone_select_test_sid').html('<div class="apolog">'+te+'</div>');
    jQuery(this).parents('.oprone_select_test_values').next('.oprone_select_test_sid').addClass('active');
    jQuery(this).parents('.oprone_select').find('.slecom').addClass('active');
    jQuery(this).parents('.oprone_select_test_values').next('.oprone_select_test_sid').attr('data-c', cla);
    
    
    
    
})


jQuery('.vih').click(function(){
    if (jQuery(this).hasClass('active')) {
        jQuery(this).parents('.viewopen').find('.hiddanno').removeClass('active');
    jQuery(this).html('Показать краткое содержимое');
    jQuery(this).removeClass('active');
    } else {
        jQuery(this).parents('.viewopen').find('.hiddanno').addClass('active');
    jQuery(this).html('Скрыть краткое содержимое');
    jQuery(this).addClass('active');
    }
    
})



jQuery('.vba').click(function(){
    jQuery('.seds').addClass('active');
    jQuery(this).remove();
    return false;
})



   
    
 

function validcap() {
       var code = jQuery('#capl').val();
      if (code != '') {
      jQuery.post('/wp-admin/admin-ajax.php', {async: false, 'action':'validcap', 'cap':code  }, function(response){
           if (response ==  0) {
                succeed = 1;
          } else {
              succeed = 0 ;
          }
         
     });
 } else {
     succeed = 1;
 }
     return succeed;
 }    
    










   
    
    
    
    jQuery('.zsend').click(function(){
       if ( validatesed() ) { 
          event.preventDefault();
      } else {
          
     var code = jQuery('#capl').val();
     jQuery.post('/wp-admin/admin-ajax.php', {async: false, 'action':'validcap', 'cap':code  }, function(response){
           if (response ==  0) {
               jQuery('.caps').addClass('active');
          } else {
               jQuery('.tickonehs').addClass('active');
    jQuery('.zsend').remove();
    zreg = jQuery('.zreg').val();
    zfio = jQuery('.zfio').val();
    zname = jQuery('.zname').val();
    zmid = jQuery('.zmid').val();
    zphone = jQuery('.zphone').val();
    zto = jQuery('.zto').val();
    zstatus = jQuery('.zstatus').val();
    zorg = jQuery('.zorg').val();
    zmess = jQuery('.zmess').val();
      jQuery.post('/wp-admin/admin-ajax.php', {'action':'seds', 'zreg':zreg, 'zfio':zfio, 'zname':zname, 'zmid':zmid, 'zphone':zphone, 'zto':zto, 'zstatus':zstatus, 'zorg':zorg, 'zmess':zmess}, function(response){
          
                   document.location.href="/thnsed";
      //jQuery('.tickonehs').removeClass('active'); 
      //jQuery('.sedone').html('');
      //jQuery('.sedone').html('<h3>Ваша заявка успешно отправлена</h3>');
         
    });
          }      
      });    
  
    
      }
     })
    
    
    
    
     function validatesed() {
    jQuery('.errsed').removeClass('active');
     
    zreg = jQuery('.zreg').val();
    zfio = jQuery('.zfio').val();
    zname = jQuery('.zname').val();
    zmid = jQuery('.zmid').val();
    zphone = jQuery('.zphone').val();
    zkud = jQuery('.zto').val();
    
     zstatus = jQuery('.zstatus').val();
     zmess = jQuery('.zmess').val();
     
    var er_zreg = false;
    var er_zfio = false;
    var er_zname = false;
    var er_kud = false;
    var er_zphone = false;
    var er_zto = false;
    var er_zstatus = false;
    var er_zmess = false;
    var er_cap = false;
    var er_agr = false;
    
     if (zreg == 0) { var   er_zreg = true; jQuery('.pol1').addClass('active');}
    if (zkud == 0) { var   er_kud = true; jQuery('.pol6').addClass('active');}
    if (zfio == '') { var   er_zfio = true; jQuery('.pol2').addClass('active');}
    if (zname == '') { var   er_zname = true; jQuery('.pol3').addClass('active');}
    if (zphone == '') { var   er_zphone = true; jQuery('.pol5').addClass('active');}
   if (zstatus == 0) { var   er_zstatus = true; jQuery('.pol7').addClass('active');}
   
    if (zmess == '') { var   er_zmess = true; jQuery('.pol9').addClass('active');}
     if (!jQuery('.agreesed').hasClass('active')) { var   er_agr = true; jQuery('.agrs').addClass('active');}
    
     var code = jQuery('#capl').val();
     jQuery.post('/wp-admin/admin-ajax.php', {async: false, 'action':'validcap', 'cap':code  }, function(response){
           if (response ==  0) {
                 er_cap = false;
              jQuery('.caps').addClass('active');
          } else {
              jQuery('.caps').removeClass('active');    
               var er_cap = true;
          }
     });
    
     
    
                return ( er_zreg || er_zfio || er_zname || er_kud || er_zphone ||  er_zstatus  || er_zmess  || er_cap || er_agr  );
  }
   
    
    




    jQuery('.caplo img').click(function(){
     jQuery.post('/wp-admin/admin-ajax.php', {'action':'chcap',  'uid':1   }, function(response){
                   jQuery('.caplo img').attr('src' , 'data:image/jpg;base64,' + response);
     
     });
    });
    

 
    

jQuery('.upn').click(function(){
   dmax  = parseInt(jQuery('#hidden').attr('data-max'));
   dval  = parseInt(jQuery('#hidden').val());
   if (dval == 1) {
       dnex = dmax;
   } else {
       dnex = dval - 1;
   }
   jQuery('#licstcount').html(dnex);
   jQuery('#hidden').val(dnex);
   jQuery('html, body').animate({
                    scrollTop: jQuery(".fime"+dnex).offset().top
                }, 1000);
});


jQuery('.dwn').click(function(){
   dmax  = parseInt(jQuery('#hidden').attr('data-max'));
   dval  = parseInt(jQuery('#hidden').val());
   if (dval == dmax) {
       dnex = 1;
   } else {
       dnex = dval + 1;
   }
   jQuery('#licstcount').html(dnex);
   jQuery('#hidden').val(dnex);
   
    jQuery('html, body').animate({
                    scrollTop: jQuery(".fime"+dnex).offset().top
                }, 1000);
   
   
});



})

 
