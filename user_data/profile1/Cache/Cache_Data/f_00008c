    uid = jQuery(this).attr('data-u');
    comm = jQuery(this).parents('.oprone_select').find('.iarea').val();
    atr = jQuery('.oprone_select_test_sid.active').attr('data-c');
    uris =  jQuery(this).attr('data-page');
    jQuery(this).parents('.slecom').children('.thanks').addClass('active');
    jQuery(this).parents('.slecom_titul_sub').prev('.orde').addClass('hidd');
    jQuery(this).parents('.slecom_titul_sub').addClass('hidd');
    
    
    jQuery.post('/wp-admin/admin-ajax.php', {'action':'polls', 'poll':poll, 'atr':atr, 'uid':uid, 'comm':comm, 'uris':uris }, function(response){
          console.log(response);
           
    });
    
    
    
    return false;
})


