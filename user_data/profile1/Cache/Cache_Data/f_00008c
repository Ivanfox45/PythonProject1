    });
    return false;
})


jQuery('#savesf').click(function(){
   idk = jQuery(this).attr('data-e');
   prich = jQuery(this).parents('.closes_inner').find('textarea').val();
   savesf = jQuery(this).parent('.saveme').prev('#resultffull2').find('.ingidfil').val();
   var err = 0;
   var errf = 0;
   if (prich == '') {
       var err = 1; jQuery('.errpri').addClass('active');
   }
   
   if (err == 0 ) {
   jQuery.post('/wp-admin/admin-ajax.php', {'action':'saveprikaz', 'idk':idk, 'savesf':savesf, 'prich':prich }, function(response){
       document.location.href="";
   });     
   }
    return false;
})

jQuery(document).on('change', '#videxp', function(){
    rutval = jQuery('#videxp option:selected').val(); 
     if (rutval == 'Экспертиза при осуществлении лицензионного контроля деятельности по производству лекарственных средств для ветеринарного применения') {var nnu = 1;}
     if (rutval == 'Экспертиза при осуществлении федерального государственного контроля (надзора) в сфере обращения лекарственных средств для ветеринарного применения') {var nnu = 2;}
     if (rutval == 'Экспертиза при осуществлении федерального государственного контроля (надзора) в области обращения с животными в части соблюдения требований к содержанию и использованию животных в культурно-зрелищных целях') {var nnu = 3;}
     if (rutval == 'Экспертиза при осуществлении федерального государственного ветеринарного контроля (надзора)') {var nnu = 4;}
     if (rutval == 'Экспертиза при осуществлении федерального государственного земельного контроля (надзора) в отношении земель сельскохозяйственного назначения, оборот которых регулируется Федеральным законом от 24 июля 2002 г. N 101-ФЗ "Об обороте земель сельскохозяйственного назначения", и виноградопригодных земель') {var nnu = 5;}
     if (rutval == 'Экспертиза при осуществлении федерального государственного карантинного фитосанитарного контроля (надзора)') {var nnu = 6;}
     if (rutval == 'Экспертиза при осуществлении федерального государственного надзора в области семеноводства в отношении семян сельскохозяйственных растений') {var nnu = 7;}
     if (rutval == 'Экспертиза при осуществлении федерального государственного контроля (надзора) в области безопасного обращения с пестицидами и агрохимикатами') {var nnu = 8;}
     if (rutval == 'Экспертиза при осуществлении федерального государственного контроля (надзора) в области обеспечения качества и безопасности зерна и продуктов переработки зерна') {var nnu = 9;}
     jQuery.post('/wp-admin/admin-ajax.php', {'action':'currnum', 'o': 1 }, function(response){
           var ye = jQuery('#ye').val();
         var yt = jQuery('#param10').val();
         if (yt != '' ) {
         spl = yt.split('.');
          var st = nnu+'-'+spl[2] + '-' + response;
         jQuery('#param1').val(st);
         }
         
     });
     
  });
  
  
  
