	});

  jQuery('.elements-block-date').click(function(){
       jQuery('.calendars, .ffn').addClass('active');
       })
       jQuery('.ffn, .closes, .reset').click(function(){
       jQuery('.calendars, .ffn').removeClass('active');
       });   
          
function vet() {
    pet = jQuery('#ss1').val();
    oper = jQuery('#ss2').val();
    col = jQuery('#ss3').val();
    country = jQuery('#ss4').val();
    
    date1 = jQuery('#vak').val();
    date2 = jQuery('#vak11').val();
    date3 = jQuery('#vak12').val();
    
    d_spl1 = date1.split('-');
    d_spl2 = date2.split('-');
    d_spl3 = date3.split('-');
    tps = jQuery('.step_line_page_3 .select_modul_param_allp_one.active').attr('data-p');
    
    if (tps == 'in') { ut = 'Ввоз в Российскую Федерацию'; }
    if (tps == 'out') { ut = 'Вывоз из Российской Федерации'; }
    if (tps == 'inner') { ut = 'Перемещение по территории Российской Федерации'; }
    
    jQuery('#tt4').html(d_spl1[2]+'.'+d_spl1[1]+'.'+d_spl1[0]);
    jQuery('#tt5').html(d_spl2[2]+'.'+d_spl2[1]+'.'+d_spl2[0]);
    jQuery('#tt6').html(d_spl3[2]+'.'+d_spl3[1]+'.'+d_spl3[0]);
    jQuery('#vlso').html(ut);
    
    
    
    jQuery.post('/wp-admin/admin-ajax.php', {'action':'vetpro', 'pet':pet, 'country':country, 'oper':oper, 'col':col, 'date2':date2, 'date1':date1, 'date3':date3
        }, function(response){
          jQuery('.totalig_id').html(response);
          
     }); 
}  
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
      jQuery('.bb1, .bb3').click(function(){
      jQuery('.step_line_page_1').removeClass('active');
      jQuery('.step_line_page_2').addClass('active');
      return false;
  })
  
  
  function validates() {
      
      
      
      
      ddf = jQuery('#ddf option:selected').val();
      dmf = jQuery('#dmf option:selected').val();
      dyf = jQuery('#dyf option:selected').val();
      
      
      ddf11 = jQuery('#ddf11 option:selected').val();
      dmf11 = jQuery('#dmf11 option:selected').val();
      dyf11 = jQuery('#dyf11 option:selected').val();
      
      ddf12 = jQuery('#ddf12 option:selected').val();
      dmf12 = jQuery('#dmf12 option:selected').val();
      dyf12 = jQuery('#dyf12 option:selected').val();
      
      
      
      jQuery('.select_modul_param select').each(function(){
      err = 0;
      ddf = jQuery('#ddf option:selected').val();
      dmf = jQuery('#dmf option:selected').val();
      dyf = jQuery('#dyf option:selected').val();
      
      
      ddf11 = jQuery('#ddf11 option:selected').val();
      dmf11 = jQuery('#dmf11 option:selected').val();
      dyf11 = jQuery('#dyf11 option:selected').val();
      
      ddf12 = jQuery('#ddf12 option:selected').val();
      dmf12 = jQuery('#dmf12 option:selected').val();
      dyf12 = jQuery('#dyf12 option:selected').val();
           
      if (ddf == 0) { jQuery('#ddf').next('.errors').addClass('active');  err = 1; return false; }
      if (dmf == 0) { jQuery('#dmf').next('.errors').addClass('active');  err = 1; return false; }
      if (dyf == 0) { jQuery('#dyf').next('.errors').addClass('active');  err = 1; return false; }
      
      
      if (ddf11 == 0) { jQuery('#ddf11').next('.errors').addClass('active');  err = 1; return false; }
      if (dmf11 == 0) { jQuery('#dmf11').next('.errors').addClass('active');  err = 1; return false; }
      if (dyf11 == 0) { jQuery('#dyf11').next('.errors').addClass('active');  err = 1; return false; }
      
      
      if (ddf12 == 0) { jQuery('#ddf12').next('.errors').addClass('active');  err = 1; return false; }
      if (dmf12 == 0) { jQuery('#dmf12').next('.errors').addClass('active');  err = 1; return false; }
      if (dyf12 == 0) { jQuery('#dyf12').next('.errors').addClass('active');  err = 1; return false; }
      
      });
      
      
      
      if (err == 0 ) {
          
          
          
          pos1 = jQuery('#pos1').val();
          pos2 = jQuery('#pos2').val();
          pos3 = jQuery('#pos3').val();
          
          st1d = jQuery('#ddf').val();
          st1m = jQuery('#dmf').val();
          st1y = jQuery('#dyf').val();

          
          st2d = jQuery('#ddf11').val();
          st2m = jQuery('#dmf11').val();
          st2y = jQuery('#dyf11').val();
          
          st3d = jQuery('#ddf12').val();
          st3m = jQuery('#dmf12').val();
          st3y = jQuery('#dyf12').val();
          
          pos1 = st1y+'-'+st1m+'-'+st1d;
          pos2 = st2y+'-'+st2m+'-'+st2d;
          pos3 = st3y+'-'+st3m+'-'+st3d;
          
jQuery('#vak').val(pos1);
jQuery('#vak11').val(pos2);
jQuery('#vak12').val(pos3);
fulldate = jQuery('#fulldate').val();
dpos1 = new Date(pos1);          
dpos2 = new Date(pos2);          
dpos3 = new Date(pos3);          
dfull = new Date(fulldate);                    

if (dpos1 >= dfull ) {
    jQuery('#erro1').removeClass('active');
    var iot1 = 0;
} else {
    jQuery('#erro1').addClass('active');
    var iot1 = 1;
}
          
          
if (dfull  >= dpos2 ) {
    jQuery('#erro2').removeClass('active');
    var iot2 = 0;
} else {
    jQuery('#erro2').addClass('active');
    var iot2 = 1;
}          
if (dfull  >= dpos3 ) {
    jQuery('#erro3').removeClass('active');
    var iot3 = 0;
} else {
    jQuery('#erro3').addClass('active');
    var iot3 = 1;
}          
          
          
          
 

 














if (iot1 == 0 && iot2 == 0 && iot3 == 0) {

dp = jQuery('#vak').val(); 
dp1 = jQuery('#vak11').val();
dp2 = jQuery('#vak12').val();

var date2 = new Date(dp);
var date1 = new Date(dp1);
var date3 = new Date(dp2);

var timeDiff = Math.abs(date2.getTime() - date1.getTime());
var timeDiffnew = Math.abs(date2.getTime() - date3.getTime());
var diffDays = Math.ceil(timeDiff / (1000 * 3600 * 24)); 
var diffDaysnew = Math.ceil(timeDiffnew / (1000 * 3600 * 24)); 
 console.log(parseInt(diffDaysnew));
if (parseInt(diffDays) > 365 ) {
jQuery('.rederr.part1').addClass('active');
}
if (parseInt(diffDaysnew) < 20 ) {
jQuery('.rederr.part2').addClass('active');
}
jQuery('.infrontfinal').addClass('active');
}
      }
  }
    
jQuery(document).on('click', '.infrontfinal.active', function(){
    
jQuery('.step_line_page_6').removeClass('active');
jQuery('.step_line_page_7').addClass('active');

jQuery('#tt4').html(jQuery('#pos1').val());
jQuery('#tt5').html(jQuery('#pos5').val());
    vet(); 
});

   
  jQuery('.inbakstart').click(function(){
      jQuery('.steps').removeClass('active');
      jQuery('.rederr').removeClass('active');
      jQuery('.step_line_page_6').addClass('active');
      return false;
  });
  jQuery('.select_modul_param select').change(function(){
      jQuery('.errors').removeClass('active')
  });
  
  
  jQuery('.infrontfinal').click(function(){
      if (jQuery('.step_line_page_3.steps .select_modul_param_allp_one.active').attr('data-p') == 'inner') {
          jQuery('#smp').children('.ppr_all_one_titu').html('Тип операции:');
          jQuery('#smp').children('.ppr_all_one_val').html('Перемещение по территории Российской Федерации');
          
          
          jQuery('#coco').children('.ppr_all_one_titu').html('Дата самой ранней действующей вакцинации:');
          jQuery('#coco').children('.ppr_all_one_val').html(jQuery('#vak11').val());
          
          
          
          
      }
      
      if (jQuery('.step_line_page_3.steps .select_modul_param_allp_one.active').attr('data-p') == 'out') {
          jQuery('#smp').children('.ppr_all_one_titu').html('Страна, куда вывозится животное');
      }
      
      jQuery('#tt4').html(jQuery('#vak').val());
      jQuery('#tt5').html(jQuery('#vak11').val());
      
      validates();
   });
    
    
    
     
  jQuery('.infront').click(function(){
      
      if (jQuery(this).parents('.steps').hasClass('step_line_page_1')) { 
      jQuery(this).parents('.steps').removeClass('active');
      jQuery(this).parents('.steps').next().addClass('active');
      
      }
       if (jQuery(this).parents('.steps').hasClass('step_line_page_2')) {
            apt = jQuery(this).parents('.steps').find('.select_modul_param_allp_one.active').attr('data-v');
            apt1 = jQuery(this).parents('.steps').find('.select_modul_param_allp_one.active').attr('data-id');
            jQuery('#ss1').val(apt1);
            jQuery('#tt1').html(apt);
             jQuery(this).parents('.steps').removeClass('active');
      jQuery(this).parents('.steps').next().addClass('active');
        }
       
        
          if (jQuery(this).parents('.steps').hasClass('step_line_page_4')) {
            apt = jQuery(this).parents('.steps').find('.select_modul_param_allp_one.active').text();
            aptcol = jQuery(this).parents('.steps').find('.select_modul_param_allp_one.active').attr('data-col');
            jQuery('#ss3').val(aptcol);
            jQuery('#tt2').html(apt);
            jQuery(this).parents('.steps').removeClass('active');
            jQuery(this).parents('.steps').next().addClass('active');
          
            
            
            
        }
           if (jQuery(this).parents('.steps').hasClass('step_line_page_5')) {
            apt = jQuery(this).parents('.steps').find('#ddsd option:selected').text();
            apt1 = jQuery(this).parents('.steps').find('#ddsd option:selected').val();
            jQuery('#ss4').val(apt1);
            jQuery('#tt3').html(apt);
              
              jQuery(this).parents('.steps').removeClass('active');
               jQuery(this).parents('.steps').next().addClass('active');
        }
        
        if (jQuery(this).parents('.steps').hasClass('step_line_page_3')) {
            
              
            aptmex = jQuery('.step_line_page_3').find('.select_modul_param_allp_one.active').attr('data-p');
            if (aptmex == 'inner') {
               jQuery(this).parents('.steps').removeClass('active');
               jQuery('.step_line_page_6').addClass('active');
                
            } else {
               
               jQuery('#ss2').val(aptmex);
               jQuery(this).parents('.steps').removeClass('active');
               jQuery(this).parents('.steps').next().addClass('active');
               
            }
            
            
            
            
            
        }
        
      
     
      return false;
  })
      
  jQuery('.inbak').click(function(){
      jQuery(this).parents('.steps').removeClass('active');
      jQuery(this).parents('.steps').prev().addClass('active');
      return false;
  })
      
    
var dateToday = new Date();
var dates = jQuery("#vak").datepicker({
    defaultDate: "+1w",
    minDate: dateToday,
    
});
   
var dates = jQuery("#vak11").datepicker({
    defaultDate: "+1w",
    maxDate: dateToday,
    
});
var dates = jQuery("#vak12").datepicker({
    defaultDate: "+1w",
    maxDate: dateToday,
    
});
 
    
  
    
    
jQuery('#ddsd').change(function(){
            apt = jQuery('#ddsd option:selected').text();
            apt1 = jQuery('#ddsd option:selected').val();
            jQuery('#ss4').val(apt1);
            jQuery('#tt3').html(apt);
})

    jQuery('.select_modul_param_allp_one').click(function(){
    
        if (jQuery(this).parents('.steps').hasClass('step_line_page_3')) {
            
            aptmex = jQuery(this).attr('data-p');
            jQuery('#ss2').val(aptmex);
             if (aptmex == 'in') {
                jQuery('.step_line_page_5').find('.select_modul_param_titul').html('Выберите страну, откуда ввозится животное');
                
            }  
            if (aptmex == 'out') {
               jQuery('.step_line_page_5').find('.select_modul_param_titul').html('Выберите страну, куда вывозится животное');
            }  
            
        }
        
        if (jQuery(this).parents('.steps').hasClass('step_line_page_2')) {
            apt = jQuery(this).attr('data-v');
            apt1 = jQuery(this).attr('data-id');
            jQuery('#ss1').val(apt1);
            jQuery('#tt1').html(apt);
            
        }
        
        if (jQuery(this).parents('.steps').hasClass('step_line_page_4')) {
            apt = jQuery(this).text();
            apt1 = jQuery(this).attr('data-col');
            jQuery('#ss3').val(apt1);
            jQuery('#tt2').html(apt);
        }
         
        if (jQuery(this).parents('.steps').hasClass('step_line_page_6')) {
            apt = jQuery(this).text();
            jQuery('#ss5').val(apt);
            jQuery('#tt5').html(apt);
        }
        
        jQuery(this).parent('.select_modul_param_allp').find('.select_modul_param_allp_one').removeClass('active');
        jQuery(this).addClass('active');
    })
    
    
    
    
    
    
    
    
    
    
    
    
    jQuery(window).scroll(function(){

		if(jQuery(window).scrollTop() > 70) {
jQuery('#backtotop').attr('style', 'display: inline-block; opacity: 1;');
			

		} else {
jQuery('#backtotop').attr('style', 'display: none; opacity: 1;');
			

		}

	});
    
       jQuery.datepicker.regional['ru'] = {

	closeText: 'Закрыть',
	prevText: 'Предыдущий',
	nextText: 'Следующий',
	currentText: 'Сегодня',
	monthNames: ['Январь','Февраль','Март','Апрель','Май','Июнь','Июль','Август','Сентябрь','Октябрь','Ноябрь','Декабрь'],
	monthNamesShort: ['Янв','Фев','Мар','Апр','Май','Июн','Июл','Авг','Сен','Окт','Ноя','Дек'],
	dayNames: ['воскресенье','понедельник','вторник','среда','четверг','пятница','суббота'],
	dayNamesShort: ['вск','пнд','втр','срд','чтв','птн','сбт'],
	dayNamesMin: ['Вс','Пн','Вт','Ср','Чт','Пт','Сб'],
	weekHeader: 'Не',
	dateFormat: 'yy-mm-dd',
	firstDay: 1,
	isRTL: false,
	showMonthAfterYear: false,
	yearSuffix: ''

};

jQuery.datepicker.setDefaults(jQuery.datepicker.regional['ru']);
      
