  /*Блок тикетов*/  
    
    
  function valtick() {
    
    var ered0 = false;      
    var ered1 = false;   
    var ered2 = false;   
    var ered3 = false; 
    var ered11 = false; 
    var ered11 = false; 
    var rok = 1;
    jQuery('#allticks .errticket').removeClass('active');  
    
    var editor0 = jQuery('#allticks .tickone').find('.tzrazd');
    var editor1 = jQuery('#allticks .tickone').find('.tzr');
    var editor2 = jQuery('#allticks .tickone').find('.adrpage');
    var editor3 = tinyMCE.activeEditor.getContent();
    var editor11 = jQuery('#allticks .tickone').find('.nmpage');
        
     if (editor0.val() == 0) { var   ered0 = true; editor0.next('.errticket').addClass('active'); } 
    if (editor1.val() == 0) { var   ered1 = true; editor1.next('.errticket').addClass('active'); }
    if (editor11.val() == 0) { var   ered11 = true; editor11.next('.errticket').addClass('active'); }
    if (editor2.val() == '') { var   ered2 = true; editor2.next('.errticket').addClass('active'); }
 //   if (editor3 != '') { var   ered3 = true; editor3.next('.errticket').addClass('active'); }
    
    
     
    return ( ered0 || ered1 || ered2 ||  ered11 );
       
     
    
  }

jQuery('.nosave').click(function(){
    jQuery('.nmz, .adrpage, .descpage, .descpagesr').val('');
  //  jQuery('.resultffullme').html('');
    jQuery('.tzrazd option[value=0], .tzr option[value=0]').prop('selected','selected');
    jQuery('.ticketadd').removeClass('active');
    return false;
})
   
   
   
   jQuery(document).on('click','.inputsub',function(){
   
   if ( valtick() ) { // если есть ошибки возвращает true
      event.preventDefault();
    } else {
      jQuery('.inputsub, .nosave').remove(); 
      jQuery('.tickonehs').addClass('active'); 
         var items = [];
      var uid = jQuery('.uid').val();
      jQuery("#newticket .tickone").each(function(){
        var tzrazd = jQuery(this).find('.tzrazd').val();
        var tzr = jQuery(this).find('.tzr').val();
        var adrpage = jQuery(this).find('.adrpage').val();
        var psr = jQuery(this).find('.psr').val();
        var descpagesr = jQuery(this).find('.descpagesr').val();
        var descpage = jQuery(this).find('#descpage').val();
        var fullimg = '';
        jQuery('.full').each(function(){
            at = jQuery(this).val();
            fullimg += at;
        });
        var descpage = jQuery(this).find('.descpage').val();
        var nmz = jQuery(this).find('.nmz').val();
        var editor3 = tinyMCE.activeEditor.getContent();
       
        
        items.push({tzrazd: tzrazd, nmz: nmz, tzr: tzr, adrpage: adrpage, psr: psr, descpagesr: descpagesr, descpage: editor3, fullimg: fullimg});
      });
     data = JSON.stringify(items);
     
     jQuery.ajax({
            url : '/wp-admin/admin-ajax.php',
            type : 'POST',
              data : {
                action : 'ticket',
                data : data,
                uid : uid,
            },
            success : function( response ) {
                //jQuery('.output').html(response);
                        document.location.href="https://fsvps.gov.ru/zajavki-sapport/";
            }
        });
    }
   
   
     
      return false;
});      
    
    jQuery('.newticket').click(function(){
    jQuery('.ticketadd').addClass('active');
    return false;
})
jQuery('.nosavetick').click(function(){
    jQuery('.ticketadd').removeClass('active');
    return false;
})
    

jQuery('.addrazd').click(function(){
   
    var hts = jQuery('#cleart').html();
    jQuery('#allticks').append(hts);
    numt();
    
    return false;
})    
  
jQuery(document).on('click','.ticket_del a',function(){
    jQuery(this).parents('.tickone').remove();
    numt();
    return false;
});
  
  
function numt() {
    cou = 1;
    jQuery('#allticks .tnum').each(function(){
        jQuery(this).html(cou);
        cou = cou + 1;
    });
}
    
    
    
 
    
   jQuery(document).on('change','.psr',function(){
       vals = jQuery(this).val();
       if (vals == 'Срочная') {
           jQuery('.ons').addClass('active');
       } else {
           jQuery('.ons').removeClass('active');
       }
   })
   
   
   
jQuery(document).on('click','.infilestick',function(){
            //  jQuery('.resultffullme').html('');
    jQuery(this).next('.jsfilesme').click();
    jQuery(this).parents('.tickone').addClass('active');
 return false;       
})
     
jQuery(document).on('change','.tickone.active .jsfilesme',function(){
 
	if (window.FormData === undefined) {
		alert('В вашем браузере FormData не поддерживается')
	} else {
		var formData = new FormData();
		jQuery.each(jQuery(".tickone.active .jsfilesme")[0].files,function(key, input){
			formData.append('file[]', input);
		});
 
		jQuery.ajax({
			type: "POST",
			url: '/tick/uptick.php',
			cache: false,
			contentType: false,
			processData: false,
			data: formData,
			dataType : 'json',
			success: function(msg){
                                        
  					jQuery('.tickone.active .resultffullme').append(msg);
                                        jQuery('.tickone').removeClass('active');
                                        
 			}
		});
	}
});      
     
     
       jQuery('.oprt').click(function(){
    uid = jQuery(this).attr('data-u');
    comm = jQuery('#newopen').val();
    
    
    jQuery.post('/wp-admin/admin-ajax.php', {'action':'newopen',  'uid':uid, 'comm':comm  }, function(response){
            jQuery('.modareasline').remove();
            jQuery('.okes').removeClass('hidd');
    });
    
    
    
    return false;
})
