 jQuery("#js-file1").change(function(){
	if (window.FormData === undefined) {
		alert('В вашем браузере FormData не поддерживается')
	} else {
		var formData = new FormData();
		jQuery.each(jQuery("#js-file1")[0].files,function(key, input){
			formData.append('file[]', input);
		});
 
		jQuery.ajax({
			type: "POST",
			url: '/reestr/uploadmlt.php',
			cache: false,
			contentType: false,
			processData: false,
			data: formData,
			dataType : 'json',
			success: function(msg){
				msg.forEach(function(msg) {
					jQuery('#resultffull').append(msg);
                                        jQuery('.errfile').removeClass('active');
                                        
 				});
			}
		});
	}
});   



 jQuery("#js-file2").change(function(){
	if (window.FormData === undefined) {
		alert('В вашем браузере FormData не поддерживается')
	} else {
		var formData = new FormData();
		jQuery.each(jQuery("#js-file2")[0].files,function(key, input){
			formData.append('file[]', input);
		});
 
		jQuery.ajax({
			type: "POST",
			url: '/reestr/uploadmlt.php',
			cache: false,
			contentType: false,
			processData: false,
			data: formData,
			dataType : 'json',
			success: function(msg){
				msg.forEach(function(msg) {
					jQuery('#resultffull2').append(msg);   jQuery('.saveme').addClass('active');
                                        jQuery('.infilesno').addClass('hidd');
 				});
			}
		});
	}
});   



 
  jQuery('#newexpert').on('submit', function(event) {
    if ( validateForm() ) { // если есть ошибки возвращает true
      event.preventDefault();
    } else {
   jQuery('#newexpert input[type="submit"]').remove();

    }
  });
  
  function validateForm() {
    jQuery('.errexp').removeClass('active');
    jQuery('#errdd').removeClass('active');
    namevlid = jQuery('#valid1').val();
    napvlid = jQuery('#valid2').val();
    prikvlid = jQuery('#valid3').val();
    date10vlid = jQuery('#param10').val();
    date11vlid = jQuery('#param11').val();
    vidvlid = jQuery('#videxp option:selected').val();
    obllid = jQuery('#oblexp option:selected').val();
    svat = jQuery('#svatt option:selected').val();
    srokatt  = jQuery('#srokatt').val();
    ingidfil = jQuery('.ingidfil').val();
    obraz = jQuery('#urobr option:selected').val();
    
    
    
    
    
    var ername = false;
    var ernap = false;
    var erprik = false;
    var date10 = false;
    var date11 = false;
    var errcom = false;
    var errvid = false;
    var errobl = false; 
    var errsrok = false; 
    var errsvat = false; 
    var vid = false;
    var errobr = false;
    var  errfile = false;
    var  errtel = false;
    
    
    
    
    
    if (srokatt == '') {
     var   errsrok = true; jQuery('.errsrok').addClass('active');
    }
    
    
    if (namevlid == '') {
     var   ername = true; jQuery('.err1').addClass('active');
    }
    if (napvlid == '') {
      var  ernap = true; jQuery('.err2').addClass('active');
    }
    
     if (!ingidfil || ingidfil == '') {
      var  errfile = true; jQuery('.errfile').addClass('active');
    }
    
    
    
    if (prikvlid == '') {
      var  erprik = true; jQuery('.err3').addClass('active');
    }
    if (date10vlid == '') {
       var  date10 = true; jQuery('.err10').addClass('active');
    }
    if (date11vlid == '') {
        var date11 = true; jQuery('.err11').addClass('active');
    }
    /*if (vidvlid == 'Не выбрано') {
        var vid = true; jQuery('.errvid').addClass('active');
    }
    if (obllid == 'Не выбрано') {
        var errobl = true; jQuery('.errobl').addClass('active');
    }*/
    if (svat == 'Не выбрано') {
        var errsvat = true; jQuery('.errsvat').addClass('active');
    }
    if (obraz == 'Не выбрано') {
        var errobr = true; jQuery('.errobr').addClass('active');
    }
    
    
    jQuery('#newexpert .telmask').each(function(){
        var usp = jQuery(this).val();
         if (usp == '')  {
            var errtel = true;jQuery('.errtel').addClass('active');
            
        }
    })
    
    
    
  spldate10vlid = date10vlid.split('.');
spldate11vlid = date11vlid.split('.');

ded10 = spldate10vlid[2] + '-' + spldate10vlid[1] + '-' + spldate10vlid[0];
ded11 = spldate11vlid[2] + '-' + spldate11vlid[1] + '-' + spldate11vlid[0];

var date1 = new Date(ded10);
var date2 = new Date(ded11);

if (spldate10vlid != '' && spldate11vlid != '') {
if (date1 < date2) {
    console.log('date1 меньше date2');
} else if (date1 > date2) {
    ht = 'Дата окончания аттестации не может быть меньше даты начала';
    console.log('date1 больше date2');
    jQuery('#errdd').html(ht);
    jQuery('#errdd').addClass('active');
    var errcom = true;
} else {
    console.log('date1 и date2 равны');
    ht = 'Дата окончания аттестации не может быть равна дате начала';
    jQuery('#errdd').html(ht);
    jQuery('#errdd').addClass('active');
    var errcom = true;
}
}      
    
    
    
    
    
    
    //alert(errfile + '=' + ername + '=' + ernap + '=' + erprik + '=' + date10 + '=' + date11 + '=' + errcom + '=' + errvid + '=' + errobl + '=' + errsvat  + '=' + vid + '=' + errobr );
    
    
    
  /*   
    var reg     = /^\w+([\.-]?\w+)*@(((([a-z0-9]{2,})|([a-z0-9][-][a-z0-9]+))[\.][a-z0-9])|([a-z0-9]+[-]?))+[a-z0-9]+\.([a-z]{2}|(com|net|org|edu|int|mil|gov|arpa|biz|aero|name|coop|info|pro|museum))$/i;
    var el_e    = jQuery("#email");
    var v_email = el_e.val()?false:true;
  
     
    
    */
     return ( ername || ernap || erprik || date10 || date11 || errfile || errcom || errobl || errsrok || errsvat || errobr || errtel);
  }
   
 









 jQuery('.vlids').on('submit', function(event) {
    exp = jQuery(this).attr('id');
    if ( validateOne(exp) ) { // если есть ошибки возвращает true
      event.preventDefault();
    }
  });
  
  function validateOne(exp) {
    
    var exp = exp;
    jQuery('.editerror').removeClass('active');
    
    expertform = jQuery('#'+exp);
    jQuery('#'+exp).find('.comdateerr').removeClass('active');
    editor0 = jQuery('#'+exp).find('.editor0').val();
    editor1 = jQuery('#'+exp).find('.editor1').val();
    editor2 = jQuery('#'+exp).find('.editor2').val();
    editor3 = jQuery('#'+exp).find('.editor3').val();
    editor6 = jQuery('#'+exp).find('.editor6').val();
    editor7 = jQuery('#'+exp).find('.editor7').val();
    edfio = jQuery('#'+exp).find('.eddnames').val();
     
    vidvlid = jQuery('#videxp option:selected').val();
    
    var ered0 = false;
    var ered1 = false;
    var ered2 = false;
    var ered3 = false;
    var ered6 = false;
    var ered7 = false;
    var errcom = false;
    var errtel = false;
    var errfio = false;
    
    
    
    if (editor0 == 'Выбрать') {
     var   ered0 = true; jQuery('.ed0').addClass('active');
    }
    if (edfio == '') {
     var   errfio = true; jQuery('.edff').addClass('active');
    }
    
    
    
    
    if (editor1 == '') {
     var   ered1 = true; jQuery('.ed1').addClass('active');
    }
    if (editor2 == '') {
     var   ered2 = true; jQuery('.ed2').addClass('active');
    }
    if (editor3 == '') {
     var   ered3 = true; jQuery('.ed3').addClass('active');
    }
    if (editor6 == '') {
     var   ered6 = true; jQuery('.ed6').addClass('active');
    }
    if (editor7 == '') {
     var   ered7 = true; jQuery('.ed7').addClass('active');
    }
   
spleditor6 = editor6.split('.');
spleditor7 = editor7.split('.');

ded6 = spleditor6[2] + '-' + spleditor6[1] + '-' + spleditor6[0];
ded7 = spleditor7[2] + '-' + spleditor7[1] + '-' + spleditor7[0];

var date1 = new Date(ded6);
var date2 = new Date(ded7);

if (editor7 != '' && editor6 != '') {
if (date1 < date2) {
    console.log('date1 меньше date2');
} else if (date1 > date2) {
    ht = 'Дата окончания аттестации не может быть меньше даты начала';
    console.log('date1 больше date2');
    jQuery('#'+exp).find('.comdateerr').html(ht);
    jQuery('#'+exp).find('.comdateerr').addClass('active');
    var errcom = true;
} else {
    console.log('date1 и date2 равны');
    ht = 'Дата окончания аттестации не может быть равна дате начала';
    jQuery('#'+exp).find('.comdateerr').html(ht);
    jQuery('#'+exp).find('.comdateerr').addClass('active');
    var errcom = true;
}
}    
      
     
     return ( ered1 ||   ered2 || ered3 || ered6  || ered7 || ered0 || errcom || errfio);
  }











jQuery('.vidall').click(function(){
    if (jQuery(this).hasClass('active')) {
        jQuery(this).parents('.all_p').next('.infos_hidd').removeClass('active');
        jQuery(this).text('Показать полные данные');
        jQuery(this).removeClass('active');
    } else {
        jQuery(this).parents('.all_p').next('.infos_hidd').addClass('active');
        jQuery(this).text('Свернуть');
        jQuery(this).addClass('active');
    }
    
    return false;
})
jQuery('.edilist').click(function(){
        jQuery(this).parents('.views-oneexpert').find('.infos_hidd').removeClass('active');
        jQuery(this).parents('.views-oneexpert').find('.vidall').removeClass('active');
        jQuery(this).parents('.views-oneexpert').find('.vidall').text('Показать полные данные');
        jQuery(this).parents('.views-oneexpert').find('.novisform').addClass('active');
        jQuery(this).parents('.views-oneexpert').find('.vidall').addClass('hidd');
        jQuery(this).parents('.views-oneexpert').children('.parab').each(function(){
        jQuery(this).removeAttr('readonly');
        })
        
    return false;
})
